create table t_body (
	id			integer 		primary key,
	name		varchar2(500)	not null,
	addr		varchar2(500)	not null
);

create table t_arm (
	body_id		integer,
	lr			integer not null,
	leng		integer,
	primary key(body_id, lr)
);

